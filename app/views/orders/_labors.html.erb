<div id="order_labors_edit">
  <fieldset>
    <legend>Labor</legend>
    <table id="order_labors_form">
      <thead>
      <tr>
        <th>Quantity</th>
        <th>Item</th>
        <th></th>
      </tr>
      </thead>
      <tbody class="items">
      <%= f.nested_fields_for :labor_line_items do |f| %>
        <tr class="item">
          <td><%= f.text_field :quantity %></td>
          <td><%= f.select :labor_id, @labors.map { |lab| [lab.name, lab.id] } %></td>
          <td>
            <a href="#" class="remove">remove</a>
            <%= f.hidden_field :_destroy %>
          </td>
        </tr>
      <% end %>
      </tbody>
    </table>
    <a href="#" class="add">add equipment</a>
  </fieldset>
</div>